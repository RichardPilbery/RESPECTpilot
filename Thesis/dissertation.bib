	Doi = {10.1136/emj.2008.064816},
	File = {Emerg Med J-2010-Figgis-151-5.pdf:/Users/tricky/Library/Application Support/Firefox/Profiles/qsvhxdon.default/zotero/storage/PXQUWAUM/Emerg Med J-2010-Figgis-151-5.pdf:application/pdf},
	Journal = {Emergency Medicine Journal},
	Month = feb,
	Number = {2},
	Pages = {151 --155},
	Title = {Investigation of paramedics' compliance with clinical practice guidelines for the management of chest pain},
	Url = {http://emj.bmj.com/content/27/2/151.abstract},
	Urldate = {2011-12-10},
