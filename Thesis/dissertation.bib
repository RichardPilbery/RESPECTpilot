@article{rowlandson_computerized_1990,
	Abstract = {The advent of thrombolytic therapy has increased the desire for an accurate computerized recognition of the {ST} elevation associated with acute myocardial infarction ({AMI).} Quantified electrocardiographic ({ECG)} criteria for {AMI}, described in the literature and in computerized {ECG} criteria packages, have concentrated on only those leads that exhibit {ST} elevation. The accuracy of this lead-specific approach has been maximized by an analysis of the associated {ST-segment} and T wave. Further advancement of the Marquette {12SL} program's ability to classify {ST} elevation due to {AMI} has required the inclusion of those leads that are not elevated. This global approach was developed by modifying the {12SL} program so that it would properly diagnose a small training set of clinically correlated {AMI} {ECGs} that did not meet the lead-specific criteria. Further training, via an evaluation of the false-positive rate, was done with a large clinical database (\&gt;30,000 {ECGs).} Both the new and the old criteria were tested with a separate {ECG} database taken from a total of 296 patients. Of these, 77 had an {AMI} (determined via the hospital discharge diagnosis). The lead-specific criteria resulted in a sensitivity of 21\% and a specificity of 99.5\%. The global approach resulted in over twice the sensitivity (53\%), while continuing to maintain a high rate of specificity (98\%).},
	Author = {Rowlandson, Ian and Kudenchuk, Peter J. and Elko, Paul P.},
	Doi = {10.1016/0022-0736(90)90065-A},
	File = {ScienceDirect Full Text PDF:/Users/tricky/Library/Application Support/Firefox/Profiles/qsvhxdon.default/zotero/storage/3R4AITER/Rowlandson et al. - 1990 - Computerized recognition of acute infarction Crit.pdf:application/pdf;ScienceDirect Snapshot:/Users/tricky/Library/Application Support/Firefox/Profiles/qsvhxdon.default/zotero/storage/A24P98GR/002207369090065A.html:text/html},
	Issn = {0022-0736},
	Journal = {Journal of Electrocardiology},
	Pages = {1--5},
	Shorttitle = {Computerized recognition of acute infarction},
	Title = {Computerized recognition of acute infarction: Criteria advances and test results},
	Url = {http://www.sciencedirect.com/science/article/pii/002207369090065A},
	Urldate = {2012-12-06},
	Volume = {23, Supplement},
	Year = {1990},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/002207369090065A},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/0022-0736(90)90065-A}}

@misc{andersen_st_2009,
	Abstract = {The current electrocardiogram criteria for the diagnosis of transmural myocardial ischemia focus on {ST} elevations, although anterior lead {ST} depression may be indicative of posterior ischemia. {ST} elevation and depression in different leads may be considered different projections of a common {ST} injury current directed toward areas of transmural ischemia. To facilitate vectorial analysis of {ST} deviations, we present the {ST} Compass-a new tool for visualization of {ST} deviations from standard 12-lead electrocardiograms. We also present a method to estimate a vector describing the magnitude and direction of the underlying {ST} injury current based on the observed {ST} deviations. In 2 cases of acute myocardial infarction, the {ST} deviation vectors form a clear pattern in the {ST} Compass, and the spatial direction of the {ST} injury current corresponds to the location of myocardial ischemia as confirmed by single-photon emission computed tomography imaging.},
	Author = {Andersen, Mads Peter and Terkelsen, Christian J and Struijk, Johannes J},
	Keywords = {{*Body} Surface Potential Mapping/mt [Methods], {*Coronary} Stenosis/di [Diagnosis], {*Diagnosis}, Computer-Assisted/mt [Methods], {*Electrocardiography/mt} [Methods], {*Myocardial} Ischemia/di [Diagnosis], {*Myocardial} Stunning/di [Diagnosis], {*User-Computer} Interface, Computer Graphics, Coronary Stenosis/co [Complications], Humans, Male, Middle Aged, Myocardial Ischemia/et [Etiology], Myocardial Stunning/et [Etiology]},
	Title = {The {ST} Compass: spatial visualization of {ST-segment} deviations and estimation of the {ST} injury vector.},
	Year = {2009}}

