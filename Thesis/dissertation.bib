	Issn = {0735-6757},
	Journal = {The American Journal of Emergency Medicine},
	Month = sep,
	Number = {7},
	Pages = {1282--1295},
	Title = {The use of a 4-step algorithm in the electrocardiographic diagnosis of {ST-segment} elevation myocardial infarction by novice interpreters},
