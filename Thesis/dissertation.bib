@misc{peacock_retrospective_2003,
	Abstract = {The authors performed a 6-month review of heart failure patients presenting to a teaching hospital emergency department to determine the rate of positive serum myocardial infarction markers. All patients with an emergency department discharge diagnosis of heart failure were included; those with a creatinine level {\textgreater}2.0 {mg/dL} were excluded. There were 151 patients who met the entry criteria, with a mean age of 68.6 +/- 13.6 years, and 84 (56\%) were men. The mean ejection fraction was 32\%, and the mean Framingham score was 3.8 +/- 1.6. Twenty (14\%) had positive markers. Troponin T was positive in 17 (11\%), and creatine kinase was positive in nine (6\%). Both markers were positive in six (4\%). Chest pain was absent in 70\% of the positive marker group. The authors conclude that elevated cardiac markers are not rare in decompensated heart failure. These pilot data suggest these tests should be routinely obtained on heart failure patients.},
	Author = {Peacock, W Frank and Emerman, Charles E and Doleh, Mohamad and Civic, Kimberly and Butt, Saud},
	Keywords = {{*Electrocardiography}, {*Emergency} Service, Hospital, {*Heart} Failure/di [Diagnosis], {*Myocardial} Infarction/di [Diagnosis], Adult, Aged, Aged, 80 and over, Biological Markers/bl [Blood], Creatine Kinase, {MB} Form, Creatine Kinase/bl [Blood], Creatine Kinase/du [Diagnostic Use], Data Interpretation, Statistical, Diagnosis, Computer-Assisted, Diagnosis, Differential, Female, Heart Failure/bl [Blood], Humans, Incidence, Isoenzymes/bl [Blood], Isoenzymes/du [Diagnostic Use], Male, Middle Aged, Myocardial Infarction/bl [Blood], Retrospective Studies, Stroke Volume/ph [Physiology], Troponin T/bl [Blood], Troponin T/du [Diagnostic Use], Ventricular Dysfunction, Left/bl [Blood], Ventricular Dysfunction, Left/di [Diagnosis]},
