@misc{sevimli_right_2007,
	Abstract = {{BACKGROUND:} This study was planned to assess strain and strain rate properties of right ventricle in patients with {RV} myocardial infarction., {MATERIAL} {AND} {METHOD:} Thirty patients with acute inferior myocardial infarction were included in this study. The presence of right ventricular infarction in association with an inferior myocardial infarction was defined by an {ST-segment} elevation 0.1 {mV} in lead V4 R. According to this definition, 15 patients had electrocardiographic signs of inferior myocardial infarction without right ventricular infarction (group I), and 15 patients had electrocardiographic signs of inferior myocardial infarction with right ventricular infarction (group {II).} Echocardiography was performed using a Vivid 5 System ({GE} Ultrasound; Horten, Norway) and a 2.5-{MHz} transducer. 2-dimensional color doppler myocardial imaging ({CDMI)} data for longitudinal function were recorded from the {RV} free wall using standard apical view. Offline analysis of the myocardial color Doppler data for regional velocity (V), strain rate (Sr), and strain (S) curves was performed using a special software program ({EchoPac} 6.4 Vingmed, Horten, Norway). They were assessed in basal, middle and apical segments of the {RV.} The differences between different groups were assessed with the Mann-Whitney U-test. A value of P {\textless} 0.05 was considered statistically significant., {RESULTS:} Systolic tissue velocity, strain, strain rate of basal (4.8 +/- 0.8 cm/s vs 6.5 +/- 1.2 cm/s, -12 +/- 3\% vs -24 +/- 5\%, 1.28 +/- 0.3/s vs -1.9 +/- 0.4/s; P {\textless} 0.001, {\textless}0.001, {\textless}0.001, respectively) and mid (4.2 +/- 0.5 cm/s vs 5.4 +/- 0.5 cm/s, -16 +/-3\% vs -26 +/- 4\%, -1.2 +/- 0.3/s vs -2.1 +/- 0.3/s; P {\textless} 0.001, {\textless}0.001, {\textless}0.001, respectively) segments of right ventricle were significantly lower in patients with {RV} infarction than in patients without {RV} infarction. There were no differences between groups for apical strain, strain rate, and systolic tissue velocity., {CONCLUSION:} This study demonstrates that right ventricular strain and strain rate were lower in patients with left ventricular inferior wall myocardial infarction with, compared to without, right ventricular infarction.},
	Author = {Sevimli, Serdar and Gundogdu, Fuat and Aksakal, Enbiya and Arslan, Sakir and Tas, Hakan and Islamoglu, Yahya and Buyukkaya, Eyup and Gurlertop, Hanefi Yekta and Senocak, Huseyin},
	Keywords = {{*Echocardiography}, Doppler/mt [Methods], {*Image} Interpretation, Computer-Assisted/mt [Methods], {*Myocardial} Infarction/pp [Physiopathology], {*Myocardial} Infarction/us [Ultrasonography], {*Ventricular} Dysfunction, Right/pp [Physiopathology], {*Ventricular} Dysfunction, Right/us [Ultrasonography], Adult, Elasticity, Female, Humans, Male, Reference Values, Reproducibility of Results, Sensitivity and Specificity, Stress, Mechanical},
	Title = {Right ventricular strain and strain rate properties in patients with right ventricular myocardial infarction.},
	Year = {2007}}

@misc{neill_myocardial_2010,
	Abstract = {The purpose of this study was to compare semi-quantitative visual scores of perfusion, motion and thickening with an automated hypoperfusion index ({HI)} in patients with suspected acute inferolateral perfusion defects. In the absence of perfusion defects motion and thickening abnormalities were assessed. Sixty-eight patients with chest pain at rest and either {ST} depression {\textgreater}=0.1 {mV} in {\textgreater}=1 of leads I, {aVL}, V1-V6 on 12-lead {ECG} or {ST} elevation {\textgreater}=0.05 {mV} in {\textgreater}=1 posterior lead on the body surface map comprised the study population. A rest gated perfusion scan was performed within 24 h of symptoms. Scans were scored for perfusion, motion and thickening semi-quantitatively. The scores were compared to the automated {HI.} A 12 h Troponin T {\textgreater}0.09 ng/ml indicated myocardial infarction ({MI).} Sixty-five patients (96\%, 65/68) had {MI.} The summed perfusion score correlated well with the {HI} (rho = 0.90, P {\textless} 0.01) and agreement between scorers was good (kappa = 0.77, 95\% {CI} = 0.57-0.94). The summed motion score correlated with the {HI} (rho = -0.61, P {\textless} 0.01) and agreement between scorers was moderate (kappa = 0.65, 95\% {CI} = 0.52-0.79). Summed thickening score correlated with {HI} (rho = -0.67, P {\textless} 0.01) and agreement between scorers was good (kappa = 0.74, 95\% {CI} = 0.64-0.88). Of the 1156 segments assessed (68 x 17), 542 had normal perfusion. Of these normally perfused segments, 113 (21\%, 113/542) had a motion abnormality and 102 (19\%, 102/542) had a thickening abnormality. Three patients with proven myocardial infarction had normal myocardial perfusion ({HI} {\textless}= 5) but exhibited wall motion and thickening abnormalities. In conclusion, assessment of wall motion and thickening in addition to perfusion in acute myocardial perfusion imaging may improve the diagnostic sensitivity for acute {MI.} Of the scores addressing motion and thickening, interobserver agreement was better for the summed thickening score.},
	Author = {Neill, Johanne and Harbinson, Mark and Adgey, Jennifer},
	Keywords = {{*Acute} Coronary Syndrome/ri [Radionuclide Imaging], {*Coronary} Circulation, {*Myocardial} Contraction, {*Myocardial} Infarction/ri [Radionuclide Imaging], {*Myocardial} Perfusion Imaging/mt [Methods], {*Tomography}, Emission-Computed, Single-Photon, {*Ventricular} Function, Left, Acute Coronary Syndrome/pp [Physiopathology], Aged, Automation, Laboratory, Biological Markers/bl [Blood], Electrocardiography, Female, Humans, Image Interpretation, Computer-Assisted, Male, Middle Aged, Myocardial Infarction/pp [Physiopathology], Northern Ireland, Observer Variation, Predictive Value of Tests, Prognosis, Reproducibility of Results, Time Factors, Troponin T/bl [Blood]},
	Title = {Myocardial wall motion and thickening assessment in early gated {SPECT} images of acute coronary syndrome patients likely to have inferolateral perfusion defects.},
	Year = {2010}}

