	Keywords = {Comparative Studies, Electrocardiography, Electrocardiography -- Methods, Electrocardiography -- Utilization, Female, Human, Male, Myocardial Infarction -- Diagnosis, Nonexperimental Studies, prehospital care, Time Factors, Treatment Outcomes},
	Lccn = {2009236901},
	Month = sep,
	Number = {3},
	Pages = {374--377},
