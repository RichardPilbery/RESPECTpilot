	Journal = {Journal Of Cardiovascular Medicine (Hagerstown, Md.)},
	Keywords = {Angioplasty, Balloon, Coronary/*mortality, Electrocardiography*, Emergency Medical Services*, Female, Heart Catheterization, Humans, Male, Middle Aged, Multivariate Analysis, Myocardial Infarction/*diagnosis, Myocardial Infarction/*physiopathology, Myocardial Infarction/mortality, Time Factors},
	Lccn = {18475124},
	Month = jun,
