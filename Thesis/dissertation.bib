	Journal = {Journal Of Electrocardiology},
	Keywords = {Decision Making*, Electrocardiography/*utilization, Emergency Medical Technicians/*statistics \& numerical data, Female, Heart Catheterization/*utilization, Humans, Male, Middle Aged, Myocardial Infarction/*diagnosis, Myocardial Infarction/*therapy, North Carolina, Time and Motion Studies*, Triage/*methods, Triage/*statistics \& numerical data},
