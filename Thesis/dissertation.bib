	Issn = {0828-{282X}},
	Journal = {Canadian Journal of Cardiology},
	Keywords = {Acute myocardial infarction, myocardial infarction, percutaneous coronary intervention, reperfusion, Thrombolysis},
	Month = sep,
	Number = {9},
	Pages = {S323--S328},
	Title = {Evaluating the views of paramedics, cardiologists, emergency department physicians and nurses on advanced prehospital management of acute {ST} elevation myocardial infarction},
