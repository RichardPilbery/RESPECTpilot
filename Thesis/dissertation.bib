	Journal = {Emergency Medicine Journal},
	Keywords = {Aged, Aged, 80 and over, Clinical Competence, Decision making, Emergency Medical Services/*methods, Emergency Medical Services/standards, Emergency Medical Technicians*, Female, Great Britain, Humans, Male, Middle Aged, Myocardial Infarction/*therapy, Myocardial Infarction/diagnosis, Prospective Studies, Thrombolytic Therapy*, Time Factors},
	Lccn = {20682955},
