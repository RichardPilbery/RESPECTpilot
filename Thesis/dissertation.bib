	Issn = {0022-0736},
	Journal = {Journal of Electrocardiology},
	Month = jul,
	Number = {3},
	Pages = {343--345},
	Title = {Accuracy of electrocardiogram interpretation by cardiologists in the setting of incorrect computer analysis},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022073606000124},
