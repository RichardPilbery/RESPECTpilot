	Issn = {0167-5273},
	Journal = {International Journal of Cardiology},
	Keywords = {Door to balloon time, Percutaneous intervention, Prognostic factors, {ST} elevation myocardial infarction, Systematic Review},
	Month = may,
	Number = {1},
