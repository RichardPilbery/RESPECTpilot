@misc{kanei_st-segment_2010,
	Abstract = {{BACKGROUND:} {ST-segment} depression in lead {aVR} in acute inferior wall {ST-segment} elevation myocardial infarction ({STEMI)} has recently been suggested as a predictor of left circumflex ({LCx)} artery involvement. The purpose of this study is to evaluate the clinical significance of {aVR} depression during inferior wall {STEMI.}, {METHODS:} This study included 106 consecutive patients who presented with inferior wall {STEMI} and underwent urgent coronary angiogram. Clinical and angiographic findings were compared between patients with and without {aVR} depression {\textgreater} or = 0.1 {mV.}, {RESULTS:} The sensitivity and specificity of {aVR} depression as a predictor of {LCx} infarction were 53\% and 86\%, respectively. In patients with right coronary artery infarction, {aVR} depression was associated with increased cardiac enzymes and the involvement of a large posterolateral branch, which may explain the larger infarction., {CONCLUSIONS:} {ST-segment} depression in lead {aVR} in inferior wall {STEMI} predicts {LCx} infarction or larger {RCA} infarction involving a large posterolateral branch. 2010 Elsevier Inc. All rights reserved.},
	Author = {Kanei, Yumiko and Sharma, Jyoti and Diwan, Ravi and Sklash, Ron and Vales, Lori L and Fox, John T and Schweitzer, Paul},
	Keywords = {{*Coronary} Artery Disease/co [Complications], {*Coronary} Artery Disease/di [Diagnosis], {*Diagnosis}, Computer-Assisted/mt [Methods], {*Electrocardiography/mt} [Methods], {*Myocardial} Infarction/di [Diagnosis], {*Myocardial} Infarction/et [Etiology], Female, Humans, Male, Middle Aged, Reproducibility of Results, Sensitivity and Specificity},
	Title = {{ST-segment} depression in {aVR} as a predictor of culprit artery and infarct size in acute inferior wall {ST-segment} elevation myocardial infarction.},
	Year = {2010}}

