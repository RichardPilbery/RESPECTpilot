	Issn = {0022-0736},
	Journal = {Journal of Electrocardiology},
	Keywords = {Acute coronary syndrome, electrocardiogram, {ST-segment} monitoring},
	Title = {Prehospital 12-lead {ST-segment} monitoring improves the early diagnosis of acute coronary syndrome},
